{% extends "PrayerToShareCoreBundle:Layout:layout.html.twig" %}

{% block content %}
    <h2>My Prayer Groups</h2>
    <ol>
        {% for group in groups %}
            <li>{{ group.name }}</li>
        {% endfor %}
    </ol>

    <h3>Public Prayer Groups</h3>
    <ul>
        {% for group in publicGroups %}
            <li>{{ group.name }}</li>
        {% endfor %}
    </ul>
{% endblock %}
